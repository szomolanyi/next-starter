{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    },
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_PATH": "."
  }
}
