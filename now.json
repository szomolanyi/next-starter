{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "emails/**"
        ]
      }
    },
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "server.js"
    }
  ],
  "alias": ["ns-fomo.now.sh"]
}
